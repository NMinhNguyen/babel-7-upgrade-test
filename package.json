  "devDependencies": {
    "@babel/cli": "7.0.0-beta.40",
    "@babel/core": "7.0.0-beta.40",
    "@babel/helper-module-imports": "7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.1",
    "cross-env": "^5.1.4",
    "jest": "^22.4.2",
    "npm-run-all": "^4.1.2",
    "prettier": "1.11.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
